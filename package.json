{
  "main": "buildscript.js",
  "scripts": {
    "build": "run-s spago:build esbuild:build",
    "dev": "run-p spago:dev esbuild:dev serve",
    "spago:build": "spago build --purs-args '--codegen corefn'",
    "spago:dev": "spago build --purs-args '--codegen corefn' --watch",
    "esbuild:build": "NODE_ENV=production node ./build.js",
    "esbuild:dev": "NODE_ENV=development node ./build.js",
    "serve": "serve dist"
  },
  "private": true,
  "devDependencies": {
    "esbuild": "^0.12.29",
    "esbuild-plugin-purescript": "^1.1.1",
    "esbuild-svelte": "^0.6.2",
    "npm-run-all": "^4.1.5",
    "serve": "^13.0.2",
    "svelte": "^3.46.2"
  }
}
